<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<GridPane onKeyPressed="#keyPressed" fx:controller="ru.com.arcadelauncher.controllers.MainController" id="pane"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
    <children>
        <Button fx:id="start" id="transparent-button" mnemonicParsing="false" prefHeight="45.0" prefWidth="345.0" text="START" onAction="#start" />
    </children>
</GridPane>